{
  "name": "a.betaev.pub",
  "scripts": {
    "dev": "bun run init && concurrently --restart-after 5000 --restart-tries -1 -n 'vite,gqlcg,auth' 'vite' 'graphql-codegen --watch' 'scripts/dev-env.sh'",
    "deploy": "echo 'not implemented' 1>&2 && exit 1",
