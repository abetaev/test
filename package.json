    "@types/lodash": "^4.14.201",
    "@types/uuid": "^9.0.7",
    "bun-types": "latest",
    "concurrently": "^8.2.2",
    "vite": "^4.5.0",
    "vite-plugin-solid": "^2.7.2"
  },
  "peerDependencies": {
    "@parcel/watcher": "^2.3.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "comlink": "^4.4.1",
