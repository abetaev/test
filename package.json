    "bun-types": "latest",
    "concurrently": "^8.2.2",
    "vite": "^4.5.0",
    "vite-plugin-solid": "^2.7.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "@parcel/watcher": "^2.3.0"
  },
